{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Memes": {
    "TemplatesDir": null,
    "TemplatesFilter": null,
    "OutputDir": "./output/generated",
    "Generators": 10,
    "ExpireMinutes": 15
  },
  "ImageBox": {
    "InternalServicesOnly": false,
    "Parser": {
      "Bind": ":",
      "SpreadStart": "{",
      "SpreadEnd": "}",
      "Errors": {
        "AttributeMoreThanOne": false,
        "AttributeBindInvalid": false,
        "ElementInvalidChild": false,
        "ElementNotFound": false,
        "ElementMoreThanOne": false,
        "InvalidInstance": false
      }
    },
    "Render": {
      "FontSize": "16px",
      "FontFamily": "anime-ace",
      "FontFamilySrc": "https://fonts.cdnfonts.com/s/54590/animeace.woff",
      "Width": "450px",
      "MaxWidth": "2000px",
      "Height": "750px",
      "MaxHeight": "2000px",
      "Animate": false,
      "AnimateDuration": "1.5s",
      "AnimateFps": 24,
      "AnimateRepeat": 0,
      "AnimateParallelism": 12,
      "TotalMaxFrames": 200
    },
    "Requests": {
      "Cache": "./output/cache",
      "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/105.0.0.0 Safari/537.36 test/443.32",
      "AllowNetworkRequests": true,
      "AllowedDomains": [
        "https://fonts.cdnfonts.com/*"
      ]
    },
    "Scripts": {
      "Timeout": "15s",
      "RecursionLimit": 999999,
      "MemoryLimitMb": 5
    }
  }
}
